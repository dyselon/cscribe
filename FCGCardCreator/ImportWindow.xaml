<Window x:Class="FCGCardCreator.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Log In" Height="191.873" Width="300" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="Username" HorizontalAlignment="Left" Height="23" Margin="78,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="206"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Margin="78,88,0,0" VerticalAlignment="Top" Width="206" Height="20"/>
        <Label Content="Log In To Google" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="274" Height="26"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="10,51,0,0" VerticalAlignment="Top" Height="26" Width="63"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top" Height="26" Width="60"/>
        <Button x:Name="LogInButton" Content="Log In" HorizontalAlignment="Left" Margin="10,113,0,0" VerticalAlignment="Top" Width="274" Height="41" Click="LogInButton_Click"/>

    </Grid>
</Window>
